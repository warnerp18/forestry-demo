{{#each markup as |variant|}}
  <div class="component-variant">
    <div class="component-variant__heading">
      <h4 class="component-variant__title">{{variant.name}}</h4>
    </div>

    <div class="component-variant__content u-clearfix">
      {{> (lookup variant 'partial')}}
    </div>

    <pre class="content__code-block"><code data-language="html">{{escapePartial (lookup variant 'partial')}}</code></pre>
  </div>
{{/each}}
