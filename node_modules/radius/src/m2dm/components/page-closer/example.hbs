<section class="page-closer u-theme-contrast">
  <div class="page-closer__inner">
    <div class="page-closer__grid">
      <div class="page-closer__contact">
        <div class="text-block text-block--small u-mb-small">
          <h4 class="paragraph-heading paragraph-heading--medium">We&rsquo;re here to help</h4>
          <p class="text-block__paragraph">Get in touch with our support team for help or more information.</p>
        </div>
        <div class="profile">
          <div class="profile__picture">
            <img src="/m2dm-public/img/profile/face.png">
          </div>
          <div class="profile__text">
            <a class="profile__text-heading" href="tel:0800 048 2467">0800 048 2467</a>
            <p class="profile__text-copy">Need help? Give us a call.</p>
          </div>
        </div>
      </div>
      <div class="page-closer__links">
        <div class="page-closer__nav-suggestion">
          <div class="text-block text-block--xsmall">
            <h3 class="paragraph-heading paragraph-heading--small">Loan calculator</h3>
            <p class="text-block__paragraph">Use our loan calculator to see how much your business loan could cost etc etc etc etc etc etc etc.</p>
          </div>
          <a class="cta-link page-closer__link--bottom" href="#">Use the loan calculator</a>
        </div>
        <div class="page-closer__nav-suggestion">
          <div class="text-block text-block--xsmall">
            <h3 class="paragraph-heading paragraph-heading--small">How it works</h3>
            <p class="text-block__paragraph">Learn about how Funding Circle brings investors and borrowers together.</p>
          </div>
          <a class="cta-link page-closer__link--bottom" href="#">See how it works</a>
        </div>
        <div class="page-closer__nav-suggestion">
          <div class="placeholder-component"></div>
        </div>
      </div>
    </div>
  </div>
</section>

